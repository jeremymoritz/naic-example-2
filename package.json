{
  "name": "naic-example-2",
  "version": "1.1.0",
  "description": "Angular 2 Single-Page App Template for NAIC",
  "scripts": {
    "clean": "rm -r dist/*",
    "lite": "lite-server",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.common.js --progress --profile --bail",
    "dev": "concurrently \"npm run build:watch\" \"npm run lite\""
  },
  "watches": {
    "build:css": "src/**/*.scss"
  },
